<Window x:Class="Hemuppgift_WPFtemplate.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Hemuppgift_WPFtemplate"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    
    <Grid>
    <Frame x:Name="MainFrame"/>

        <Button x:Name="btnGrid" Content="Grid" Click="btnGrid_Click"/>
        <Button x:Name="btnStack" Grid.Column="1" Content="StackPanel" Click="btnStack_Click"/>
        <Button x:Name="btnWrap" Content="WrapPanel" Grid.Row="1" Click="btnWrap_Click"/>
        <Button x:Name="btnDock" Grid.Column="1" Content="DockPanel" Grid.Row="1" Click="btnDock_Click"/>
        
        <Grid.RowDefinitions>
            <RowDefinition Height="*"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="*"/>
            <ColumnDefinition Width="*"/>
        </Grid.ColumnDefinitions>
        
        
    </Grid>
</Window>
